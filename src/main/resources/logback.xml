<?xml version="1.0" encoding="UTF-8"?>

<configuration>
    <appender name="OUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-5level | %d{ISO8601} | [%-41t] | %m %logger %n</pattern>
        </encoder>
    </appender>

    <root>
        <level value="INFO"/>
        <appender-ref ref="OUT"/>
    </root>
</configuration>
